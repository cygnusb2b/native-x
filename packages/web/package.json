{
  "name": "fortnight-web",
  "version": "0.0.1",
  "main": "src/App.jsx",
  "repository": "https://github.com/cygnusb2b/fortnight-web.git",
  "author": "AS3",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "prestart": "$npm_package_docker_dev_install",
    "start": "$npm_package_docker_dev_up",
    "stop": "$npm_package_docker_dev_down",
    "poststart": "yarn stop",
    "terminal": "docker exec -it fortnightweb_app_1 /bin/bash",
    "prebuild": "$npm_package_docker_prod_install",
    "postbuild": "$npm_package_docker_prod_clean && $npm_package_docker_dev_down",
    "build": "$npm_package_docker_prod_build",
    "build:run": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production --env.prod -p"
  },
  "docker": {
    "dev": {
      "install": "docker-compose -p fortnightweb run --no-deps --entrypoint yarn app install",
      "up": "docker-compose -p fortnightweb up",
      "down": "docker-compose -p fortnightweb down"
    },
    "prod": {
      "install": "docker-compose -p fortnightweb run --no-deps --entrypoint yarn app install",
      "build": "docker-compose -p fortnightweb run --no-deps --entrypoint yarn app run build:run",
      "clean": "rm -rf node_modules",
      "down": "docker-compose -p fortnightweb down"
    }
  },
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "bootstrap": "^4.0.0",
    "es6-promise": "^4.2.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jarallax": "^1.10.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-apollo": "^2.1.2",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^3.1.1"
  }
}
