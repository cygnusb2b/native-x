<div class="sticky-top z-depth-1">
  <nav class="navbar navbar-expand navbar-light bg-white">
    <div class="navbar-brand mx-auto">
      {{#if model.logo.src}}
        <img class="filter-grayscale brand-logo" alt="{{model.name}}" title="{{model.name}}" src="{{ model.logo.src }}?h=30&dpr=2" />
      {{else}}
        <span class="font-weight-bold">{{model.name}}</span>
      {{/if}}
    </div>
  </nav>

  {{loading-progress class="bg-white"}}
</div>


